        "mainConfig": "./webpack.main.config.js",
        "renderer": {
          "config": "./webpack.renderer.config.js",
          "entryPoints": [
            {
              "html": "./src/index.html",
              "js": "./src/renderer.js",
              "name": "main_window"
            },
            {
              "html": "./src/index.html",
              "js": "./frontend/pages/downloadpage/downloadpage.jsx",
              "name": "download"
            }
          ]
        }
      }
